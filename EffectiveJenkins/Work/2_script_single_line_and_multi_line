node {
   
   echo 'Simple Pipeline....'
   
   stage ('Stage 1') {
       
       echo 'This is the first stage....'
       
       sh 'echo "This is a single line execution" '
       
       sh '''
            echo 'Multi line execution 1'
            echo 'Multi line execution 2'
            ls -lah
            dir
            uname -r
       
       '''
   }
}
